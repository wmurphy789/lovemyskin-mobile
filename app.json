{
  "expo": {
    "name": "Love My Skin",
    "slug": "lovemyskin",
    "version": "0.9.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#218381"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "dev.lovemyskin",
      "buildNumber": "0.9.0",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Tracking for Facebook login"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#218381"
      },
      "userInterfaceStyle": "light",
      "package": "dev.lovemyskin",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "androidStatusBar": {
      "backgroundColor": "#218381",
      "barStyle": "light-content",
      "translucent": false
    }
  }
}
